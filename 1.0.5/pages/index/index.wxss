.layer {
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 9999;
    background: rgba(0,0,0,0.30);
}

.layerWrap {
    z-index: 10000;
    width: 70%;
    position: fixed;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    background-color: #FFF;
    border-radius: 10px;
    text-align: center;
    padding-top: 140rpx;
}

.layerclose {
    width: 40rpx;
    height: 40rpx;
    border-radius: 50%;
    background-color: #cccccc;
    position: absolute;
    right: -10rpx;
    top: -20rpx;
}

.layerclose::after,.layerclose::before {
    content: '';
    display: block;
    width: 26rpx;
    height: 4rpx;
    background-color: #FFF;
    position: absolute;
    left: 8rpx;
    top: 18rpx;
}

.layerclose::after {
    transform: rotate(45deg);
}

.layerclose::before {
    transform: rotate(-45deg);
}

.layerimg {
    width: 400rpx;
    height: 275rpx;
    position: absolute;
    display: block;
    top: -180rpx;
    left: 50%;
    transform: translateX(-50%);
}

.layerTitle {
    text-align: center;
    font-size: 28rpx;
    font-weight: bold;
}

.layerbody {
    padding: 20rpx 0;
}

