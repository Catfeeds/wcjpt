.pricon {
    display: inline-block;
    width: 6px;
    height: 6px;
    border-top: 1px solid #FD7964;
    border-right: 1px solid #FD7964;
    transform: rotate(135deg);
    vertical-align: middle;
    margin-top: -3px;
    margin-left: 8px;
}

.setnum {
    display: flex;
}

.setnum view {
    width: 80rpx;
    height: 64rpx;
    border: 1px solid #E6E6E6;
    text-align: center;
    line-height: 64rpx;
}

.setnum .peopnum {
    width: 100rpx;
    color: #B3B3B3;
    border: 1px solid #E6E6E6;
    text-align: center;
    height: 64rpx;
    line-height: 64rpx;
}

.setnum .reduce {
    border-top-left-radius: 8rpx;
    border-bottom-left-radius: 8rpx;
    border-right: none;
    position: relative;
}

.setnum .reduce::after {
    content: '';
    display: block;
    width: 24rpx;
    height: 4rpx;
    background-color: #B3B3B3;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
}

.setnum .plus {
    border-bottom-right-radius: 8rpx;
    border-top-right-radius: 8rpx;
    border-left: none;
    position: relative;
}

.setnum .plus::after,.setnum .plus::before {
    content: '';
    display: block;
    width: 24rpx;
    height: 4rpx;
    background-color: #B3B3B3;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
}

.setnum .plus::after {
    transform: rotate(90deg);
    margin-left: -12rpx;
    margin-top: -2rpx;
}

