.head {
    background-color: #FFF;
    padding: 20rpx;
}

.prizepho {
    height: 350rpx;
    position: relative;
}

.prizepho image {
    width: 100%;
    height: 350rpx;
    display: block;
    position: relative;
}

.sponsor-zans {
    position: absolute;
    top: 0;
    right: 0;
    height: 60rpx;
    line-height: 60rpx;
    background: rgba(0,0,0,0.30);
    font-size: 24rpx;
    color: #FFFFFF;
    padding: 0 30rpx;
    border-bottom-left-radius: 8rpx;
}

.prizecs {
    margin-top: 20rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.prizecs .prizecof {
    font-size: 34rpx;
    color: #000000;
}

.prizeLi {
    font-size: 28rpx;
    color: #999999;
    flex: 1;
    text-align: right;
}

.ss-flex {
  display:flex;
  justify-content:space-between;
  line-height:36rpx;
  margin-top: 20rpx;
}

.ss-flex .ss-gray{
  color: #B3B3B3;
}

.condition {
    font-size: 24rpx;
    color: #B3B3B3;
}

.mode {
    height: 70rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0 20rpx;
    background-color: #FAFAFA;
    font-size: 24rpx;
    color: #949494;
    border-top: 1px dashed #cccccc;
    box-shadow: 0 1px 12rpx 0 rgba(149,135,134,0.20);
}

.mode .sponsor text {
    margin: 0 6rpx;
    font-weight: bold;
}

.mode .copyname image {
    width: 24rpx;
    height: 24rpx;
    margin-right: 8rpx;
    vertical-align: middle;
    margin-top: -3px;
}

.jump {
    color: #333;
}

.jump image {
    width: 26rpx;
    height: 26rpx;
    vertical-align: middle;
    margin-top: -3px;
    margin-left: 8rpx;
}

.jump .next {
    width: 10rpx;
    height: 10rpx;
    display: inline-block;
    border-top: 1px solid #CCC;
    border-right: 1px solid #CCC;
    transform: rotate(45deg);
}

.padd {
    padding: 0 20rpx;
}

.remarks {
    background-color: #F6DFC7;
    height: 90rpx;
    display: flex;
    align-items: center;
    margin-top: 60rpx;
    border-radius: 10rpx;
    color: #9C8064;
    font-size: 32rpx;
    padding-left: 30rpx;
}

.partake,.parSuccess {
    padding-top: 70rpx;
}

.partake button {
    width: 455rpx;
    background-color: #FE5A50;
    font-size: 34rpx;
    color: #FFFFFF;
    height: 120rpx;
    line-height: 120rpx;
    background-image: linear-gradient(-180deg,#F95F62 0%,#CF0F0A 100%);
    box-shadow: 0 11rpx 0 -1px #B21A17;
}

.parSuccess button {
    height: 120rpx;
    line-height: 120rpx;
    font-size: 34rpx;
    width: 455rpx;
    background: #F07575;
    color: #FFFFFF;
    font-weight: bold;
    position: relative;
}

.successIcon {
    position: absolute;
    top: 50rpx;
    left: 310rpx;
    display: block;
}

.successIcon::after {
    content: '';
    display: inline-block;
    width: 20rpx;
    height: 2px;
    background-color: #666;
    transform: rotate(45deg);
    position: absolute;
    top: 16rpx;
    left: 0;
}

.successIcon::before {
    content: '';
    display: inline-block;
    width: 36rpx;
    height: 2px;
    background-color: #666;
    transform: rotate(-45deg);
    position: absolute;
    top: 10rpx;
    left: 10rpx;
}

.moreluck {
    font-size: 24rpx;
    color: #FE5A50;
    text-decoration: underline;
    text-align: center;
    margin-top: 40rpx;
}

.group {
    display: flex;
    margin-top: 70rpx;
    align-items: center;
}

.group .btn1 {
    height: 120rpx;
    line-height: 120rpx;
    font-size: 34rpx;
    width: 455rpx;
    background: #F07575;
    color: #FFFFFF;
    font-weight: bold;
    flex-shrink: 0;
}

.groupxian {
    width: 120rpx;
    color: #F07575;
    letter-spacing: 6rpx;
    flex-shrink: 0;
}

.group .groupBtn {
    background-image: linear-gradient(-180deg,#F95F62 0%,#CF0F0A 100%);
    width: 400rpx;
    height: 120rpx;
    text-align: center;
    border-radius: 10rpx;
    color: #FFF;
    flex-shrink: 0;
}

.groupjoin {
    background-image: linear-gradient(-180deg,#F95F62 0%,#CF0F0A 100%);
    width: 400rpx;
    text-align: center;
    border-radius: 10rpx;
    margin: 70rpx auto 0;
}

.groupjoin button {
    height: 140rpx;
    background-color: transparent;
    color: #FFF;
    line-height: 30rpx;
}

.groupjoin .ones {
    font-size: 34rpx;
    margin-top: 40rpx;
}

.groupjoin .twos {
    font-size: 22rpx;
    opacity: .6;
    margin-top: 10rpx;
}

.groupBtn button {
    height: 120rpx;
    line-height: 120rpx;
    background-color: transparent;
    color: #FFF;
    font-size: 34rpx;
}

.groupBtn .v1 {
    font-size: 34rpx;
    margin-top: 20rpx;
}

.groupBtn .v2 {
    font-size: 22rpx;
    opacity: .6;
    margin-top: 10rpx;
}

.groupadd {
    margin-top: 40rpx;
    padding: 0 20rpx;
}

.groupfull {
    color: #FE5A50;
    font-size: 24rpx;
    text-align: center;
    margin-bottom: 20rpx;
}

.groupfull .icon {
    width: 25rpx;
    height: 25rpx;
    display: inline-block;
    border-radius: 50%;
    background-color: #FE5A50;
    vertical-align: middle;
    font-size: 20rpx;
    color: #FFF;
    line-height: 25rpx;
    margin-top: -3px;
    margin-right: 6rpx;
}

.groupnum {
    border: 1px solid #B21A17;
    border-radius: 8rpx;
    padding: 20rpx;
}

.groupnum .groupv1 {
    text-align: center;
    color: #999;
    font-size: 24rpx;
}

.groupv1::before,.groupv1::after {
    content: '';
    display: inline-block;
    width: 170rpx;
    height: 1px;
    background-color: #F1D0CD;
    vertical-align: middle;
    margin-top: -3px;
}

.groupv1::before {
    margin-right: 10rpx;
}

.groupv1::after {
    margin-left: 10rpx;
}

.groupv1 text {
    color: red;
}

.grouplist {
    margin-top: 20rpx;
    display: flex;
    justify-content: center;
    flex-wrap: wrap;
}

.grouplist image {
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
    margin-right: 20rpx;
    margin-top: 20rpx;
}

.card {
    padding: 30rpx;
    background-color: #FAFAFA;
    border-top: 1px dashed #CCCCCC;
}

.cardwrap {
    display: flex;
}

.cardwrap:nth-of-type(2) {
    margin-top: 30rpx;
}

.cardsmg {
    flex: 1;
    height: 88rpx;
    line-height: 88rpx;
    border: 1px dashed #CCCCCC;
    background-color: #F5F5F5;
    border-radius: 10rpx;
    font-size: 28rpx;
    text-align: left;
    padding-left: 10rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.cardsmg .label {
    color: #999999;
    margin-right: 20rpx;
}

.cardsmg .num {
    font-weight: bold;
    color: #333;
}

.cardcopy {
    height: 88rpx;
    line-height: 88rpx;
    margin-left: 40rpx;
    font-size: 24rpx;
    color: #CCC;
}

.havepart {
    margin-top: 40rpx;
    text-align: center;
}

.havenum {
    font-size: 24rpx;
    color: #B3B3B3;
}

.havenum text {
    margin: 0 6rpx;
    font-weight: bold;
}

.partImg {
    padding: 0 100rpx;
    display: flex;
    margin-top: 20rpx;
    justify-content: center;
    flex-wrap: wrap;
}

.partImg image {
    width: 60rpx;
    height: 60rpx;
    display: block;
    margin-right: 20rpx;
}

.explain {
    background-color: #FFF;
    padding: 30rpx;
    margin-top: 40rpx;
}

.explain .explainTitle {
    text-align: center;
    color: #9F9494;
    font-size: 28rpx;
}

